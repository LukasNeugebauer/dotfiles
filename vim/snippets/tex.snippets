snippet eq "frame for equation" b
\begin{equation}
	$1
\end{equation}
$2
endsnippet

snippet be "generic framing" b
\begin{${1:equation}}
	$2
\end{$1}
$3
endsnippet

snippet p "paragraph" b
\paragraph
endsnippet

snippet s "section" b
\section
endsnippet

snippet ss "subsection" b
\subsection
endsnippet
